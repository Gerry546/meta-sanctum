            "python": ".venv/bin/python",
            "console": "integratedTerminal",
            "args": "-v ${command:pickArgs} -u y -c n"
        },
        {
            "name": "Parse Requirement, No Clean",
            "type": "debugpy",
            "request": "launch",
            "program": "sources/meta-homeassistant/scripts/parse_requirement.py",
